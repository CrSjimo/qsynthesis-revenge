#define VERSION     @PROJECT_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_PATCH@,@PROJECT_VERSION_TWEAK@
#define FILE_NAME    "@PROJECT_EXPORT_FILENAME@.exe"
